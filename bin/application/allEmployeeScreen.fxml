<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.allEmployeesController">
   <top>
      <Label text="All registered Employees" BorderPane.alignment="CENTER">
         <font>
            <Font name="Arial Bold" size="36.0" />
         </font>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="EIDCol" prefWidth="132.8477921485901" text="Employee ID" />
          <TableColumn fx:id="NameCol" prefWidth="203.45399475097656" text="Name" />
            <TableColumn fx:id="DOBCol" prefWidth="143.5460205078125" text="DOB" />
            <TableColumn fx:id="SalaryCol" prefWidth="151.50503540039062" text="Salary" />
            <TableColumn fx:id="WarehouseCol" prefWidth="168.60885620117188" text="Warehouse ID" />
        </columns>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </TableView>
   </center>
   <bottom>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#toEmployee" prefHeight="27.0" prefWidth="142.0" text="Go back" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" top="10.0" />
         </BorderPane.margin>
      </Button>
   </bottom>
</BorderPane>
